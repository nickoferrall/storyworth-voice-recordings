{
  "name": "fitlo",
  "description": "fitlo is the ultimate way to manage functional fitness competitions",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev -p 3000\" \"npx pgtyped -w -c pgtypedConfig.json\"",
    "dev:local": "NODE_ENV=development concurrently \"next dev -p 3000\" \"npx pgtyped -w -c pgtypedConfig.json\"",
    "dev:prod": "NODE_ENV=production concurrently \"next dev -p 3000\" \"npx pgtyped -w -c pgtypedConfig.json\"",
    "setup:local": "node scripts/setup-local-dev.js",
    "env:info": "node scripts/env-info.js",
    "pg:build": "pgtyped -c ./pgtypedConfig.js",
    "pg:build:watch": "npx pgtyped -w -c pgtypedConfig.json",
    "build": "next build && next-sitemap --config next-sitemap.config.js",
    "build:ignore-types": "tsc --skipLibCheck --noEmit && next build && next-sitemap",
    "start": "next start -H 0.0.0.0 -p ${PORT:-3000}",
    "typecheck": "tsc --noEmit",
    "generate": "graphql-codegen --config codegen.yml && yarn generate:kysely-types",
    "generate:kysely-types": "kysely-codegen kysely.config.json && mv ./node_modules/kysely-codegen/dist/db.d.ts ./src/generated/database.ts",
    "generate:watch": "graphql-codegen --watch",
    "addQuery": "./createQuery.sh",
    "migrate:legacy": "echo 'Legacy Kysely migrations archived. Use yarn db:* commands for new migrations.'",
    "populate:competition": "ts-node scripts/populateCompetition.ts",
    "populate:users": "ts-node scripts/populateUsers.ts",
    "cleanup:duplicates": "node scripts/cleanupDuplicateRegistrations.js",
    "lint": "next lint",
    "test": "playwright test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:fast": "playwright test fast-health-check.spec.ts --workers=3",
    "test:fast:headed": "playwright test fast-health-check.spec.ts --headed",
    "test:full-flow": "playwright test recorded-full-flow.spec.ts",
    "test:full-flow:headed": "playwright test recorded-full-flow.spec.ts --headed",
    "test:health": "playwright test health-check.spec.ts",
    "test:critical": "playwright test health-check.spec.ts auth.spec.ts --workers=2",
    "scrape:all": "npm run scrape:aretas && npm run scrape:compCorner",
    "scrape:aretas": "NODE_ENV=production npx ts-node -r dotenv/config scrapers/aretas.ts dotenv_config_path=.env.prod",
    "scrape:compCorner": "NODE_ENV=production npx ts-node -r dotenv/config scrapers/compCorner.ts dotenv_config_path=.env.prod",
    "scrape:hyrox": "NODE_ENV=production npx ts-node -r dotenv/config scrapers/hyrox.ts dotenv_config_path=.env.prod",
    "scrape:staging": "NODE_ENV=production npx ts-node -r dotenv/config scripts/daily-scraper-cron.ts dotenv_config_path=.env.prod",
    "fix:potential-locations": "NODE_ENV=production npx ts-node -r dotenv/config scripts/fix-potential-comp-locations.ts dotenv_config_path=.env.prod",
    "extract:logos": "NODE_ENV=production npx ts-node -r dotenv/config scripts/extract-aretas-logos.ts dotenv_config_path=.env.prod",
    "compare:csvs": "ts-node scrapers/compareCsvToDb.ts",
    "sync:directory-comps": "NODE_ENV=production node scripts/sync-directory-comps.js",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase db reset --debug",
    "supabase:new": "npx supabase migration new",
    "supabase:push": "npx supabase db push --project-ref czbbldmexpsiebwhrxox",
    "supabase:local": "npx supabase db reset",
    "db:new": "npx supabase migration new",
    "db:local": "npx supabase db reset && yarn generate",
    "db:migrate": "npx supabase migration up && yarn generate",
    "db:push": "npx supabase db push --project-ref czbbldmexpsiebwhrxox",
    "db:deploy": "yarn db:push && echo 'Database deployed! Now deploy your app with: git push origin main'"
  },
